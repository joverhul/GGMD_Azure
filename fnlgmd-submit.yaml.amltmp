$schema: https://azuremlschemas.azureedge.net/latest/commandJob.schema.json
code: GGMD
command: >-
  script-fnlgmd.sh ${{inputs.smiles}} ${{inputs.total_vocab}} ${{inputs.model_zinc}} ${{outputs.generations}}
inputs:
  smiles:
    path: >-
      azureml:zinc-smiles:1
    mode: ro_mount
    type: uri_file
  total_vocab:
    path: >-
      azureml:zinc-all-vocab:1
    mode: ro_mount
    type: uri_file
  model_zinc:
    path: >-
      azureml:zinc-model-epoch35:1
    mode: ro_mount
    type: uri_file
outputs:
  generations:
    mode: rw_mount
    type: uri_folder
environment: azureml:GGMD-1:2
compute: azureml:cpu-cluster
display_name: GGMD-AMPL
experiment_name: GGMD
description: Input yaml file to create job on Microsoft Azure.